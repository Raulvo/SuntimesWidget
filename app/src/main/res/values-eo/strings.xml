    <!-- Dialog: Help -->  
    <string name="help_dialog_title">Helpo</string>
    <string name="help_appearance_title"><![CDATA[
        La jenaj mallongigoj estas disponeblaj: <br />
                * <b>%loc</b> por etikedo <br />
                * <b>%lat</b> por latitudo <br />
                * <b>%lon</b> por longitudo <br/>
                * <b>%m</b> por temp-reĝimo (mallonge) (ekz. “Civila”) <br/>
                * <b>%M</b> por temp-reĝimo (longe) (ekz. “Civila krepusko”) <br/>
                * <b>%s</b> por fonto de datumoj (ekz. “sunrisesunsetlib”) <br/>
                * <b>%t</b> por horzono (ekz. Usono/Arizono) <br/>
    ]]></string>
    
    <string name="help_general">  <!-- general app help; composites a couple help topics together. -->
        <xliff:g id="help_general_timeMode">%1$s</xliff:g><![CDATA[<br />]]>
        <xliff:g id="help_general_daylength">%2$s</xliff:g>
    </string>

    <string name="help_general_timeMode"><![CDATA[
          <b>Reala sunleviĝo/sunsubiro:</b> la Suno estas sur la horizonto, tio ĉi estas proksimuma tempo de sunleviĝo aŭ sunsubiro.<br/><br/>
          <b>Civila krepusko:</b> la Suno estas 6 gradoj sub la horizonto, la plej helaj steloj estas videblaj. Estas sufiĉe da lumo por fari plejparte da agadoj eksterdome sen artefarita prilumado.<br/><br/>
          <b>Naŭtika krepusko:</b> la Suno estas 12 gradoj sub la horizonto, konturoj de objektoj kaj la horizonto estas kvazaŭ nevideblaj. Lumo mankas por plejparte da agadoj eksterdome sen artefarita prilumado.<br/><br/>
          <b>Astronomia krepusko:</b> la Suno estas 18 gradoj sub la horizonto. Preskaŭ nigra nokto.<br/>
    ]]></string>
    <string name="help_general_daylength"><![CDATA[
        <b>Longo de la tago:</b> kvanto da tempo inter sunleviĝo kaj sunsubiro.<br/><br/>
        <b>Kiom da uzebla lumo:</b> kvanto da tempo inter civila sunleviĝo kaj civila sunsubiro.</br>
    ]]>
    </string>
    <string name="help_action_launch"><![CDATA[
        La plene unusignifa nomo de klaso de Ago por lanĉi.<br/>
    ]]></string>
    <string name="help_widgetlist"><![CDATA[
          Por <b>aldoni novan fenestraĵon</b>, iru al la ĉefekrano.<br/><br/>
          Por <b>reagordi ekzistan fenestraĵon</b>, elektu ĝin el la jena listo.<br/>
    ]]></string>

