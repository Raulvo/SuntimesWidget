    <!-- Dialog: Help -->
    <string name="help_dialog_title">Ayuda</string>
    <string name="help_appearance_title"><![CDATA[
        Las siguientes sustituciones están soportadas: <br />
                * <b>%loc</b> para etiqueta <br />
                * <b>%lat</b> para latitud <br />
                * <b>%lon</b> para longitud <br/>
                * <b>%m</b> para modo de tiempo (corto) (p.ej. "Civil") <br/>
                * <b>%M</b> para modo de tiempo (largo) (p.ej. "Crepúsculo Civil ") <br/>
                * <b>%s</b> para fuente de datos (p.ej. sunrisesunsetlib) <br/>
                * <b>%t</b> para zona horaria (id) (p.ej. ES/Madrid) <br/>
    ]]></string>

    <string name="help_general">  <!-- general app help; composites a couple help topics together. -->
        <xliff:g id="help_general_timeMode">%1$s</xliff:g><![CDATA[<br />]]>
        <xliff:g id="help_general_daylength">%2$s</xliff:g>
    </string>

    <string name="help_general_timeMode"><![CDATA[
          <b>Tiempo real:</b> El Sol está en el horizonte: esto es el tiempo aproximado de la salida y puesta de sol.<br/><br/>
          <b>Crepúsculo civil:</b> El Sol está 6 grados por debajo del horizonte; las estrellas más brillantes son visibles. Hay suficiente luz para la mayoría de actividades al aire libre sin necesidad de linterna.<br/><br/>
          <b>Crepúsculo náutico:</b> El Sol está 12 grados por debajo del horizonte; las siluetas de los objetos y el horizonte son difíciles de distinguir. No hay suficiente luz para actividades al aire libre sin linterna.<br/><br/>
          <b>Crepúsculo astronómico:</b> El Sol está 18 grados por debajo del horizonte. Casi la oscuridad de la noche.<br/>
    ]]></string>
    <string name="help_general_timeMode2"><![CDATA[
          <b>Equinocio primaveral:</b> TODO.<br/><br/>
          <b>Solsticio de verano:</b> TODO.<br/><br/>
          <b>Equinocio de otoño:</b> TODO.<br/><br/>
          <b>Solsticio de invierno:</b> TODO.<br/>
    ]]></string>
    <string name="help_general_daylength"><![CDATA[
        <b>Duración del día:</b> la cantidad de tiempo entre la salida y la puesta de Sol.<br/><br/>
        <b>Luz usable:</b> la cantidad de tiempo entre el crepúsculo civil diurno y el nocturno.</br>
    ]]>
    </string>
