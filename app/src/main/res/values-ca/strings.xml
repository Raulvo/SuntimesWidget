    <!-- Dialog: Help -->
    <string name="help_dialog_title">Ajuda</string>
    <string name="help_appearance_title"><![CDATA[
    Les següents substitucions estàn soportades: <br />
            * <b>%loc</b> per a etiqueta <br />
            * <b>%lat</b> per a latitud <br />
            * <b>%lon</b> per a longitud <br/>
            * <b>%m</b> per a mode de temps (curt) (p.ex. "Civil") <br/>
            * <b>%M</b> per a mode de temps (llarg) (p.ex. "Crepuscle Civil ") <br/>
            * <b>%s</b> per a font de dades (p.ex. sunrisesunsetlib) <br/>
            * <b>%t</b> per a zona horària (id) (p.ex. ES/Barcelona) <br/>
]]></string>

    <string name="help_general">  <!-- general app help; composites a couple help topics together. -->
        <xliff:g id="help_general_timeMode">%1$s</xliff:g><![CDATA[<br />]]>
    <xliff:g id="help_general_daylength">%2$s</xliff:g>
</string>

    <string name="help_general_timeMode"><![CDATA[
      <b>Temps real:</b> El Sol està en el horizonte: això és el temps  aproximat de la sortida i posta de sol.<br/><br/>
      <b>Crepuscle civil:</b> El Sol està 6 graus per sota de l\'horitzó; les estrelles més brillants són visibles. Hi ha suficient llum per a la majoría de les activitats a l\'aire lliure sense necessitat de llanterna.<br/><br/>
      <b>Crepuscle nàutic:</b> El Sol està 12 graus per sota de l\'horitzó; les siluetes dels objectes i l\'horitzó les difíciles de distinguir. No hi ha suficient llum per a activitatss a l\'aire lliure sense llanterna.<br/><br/>
      <b>Crepuscle astrònomic:</b> El Sol està 18 graus per sota de l\'horitzó. Qüasi l'obscuritat de la nit.<br/>
]]></string>
    <string name="help_general_timeMode2"><![CDATA[
      <b>equinocci primaveral:</b> TODO.<br/><br/>
      <b>solstici d\'estiu:</b> TODO.<br/><br/>
      <b>equinocci de tardor:</b> TODO.<br/><br/>
      <b>solstici d\'hivern:</b> TODO.<br/>
]]></string>
    <string name="help_general_daylength"><![CDATA[
    <b>Duració del dia:</b> la quantitat de temps entre la sortida i la posta de Sol.<br/><br/>
    <b>Llum usable:</b> la quantitat de temps entre el crepuscle civil diürn i el nocturn.</br>
]]>
</string>
